public class RandomGenerator {

    public static void main(String[] args) {
	// write your code here
        Random rn=new Random();
        int numberWhithClassRandom=rn.nextInt(50);
        System.out.println(numberWhithClassRandom);
        int numberWhithClassMath=(int)(Math.random()*100);
        System.out.println(numberWhithClassMath);
        int numberWhithBothMethod=rn.nextInt(50)+(int)(Math.random()*100);
        System.out.println(numberWhithBothMethod);
        int sumOfRandomNumber=numberWhithClassRandom+numberWhithBothMethod+numberWhithClassMath;
        System.out.println(sumOfRandomNumber);

    }
}
