package by.lukash.HW6Task1;

import by.lukash.HW6Task1.bean.DoWhileOddChecker;
import by.lukash.HW6Task1.bean.ForOddChecker;
import by.lukash.HW6Task1.bean.WhileOddChecker;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class Hw6Task1Application {


	public static class CheckersDemo {
		public static void main(String[] args) {
			Thread thread = new Thread(new DoWhileOddChecker());
			thread.start();
			Thread thread1 = new Thread(new ForOddChecker());
			thread1.start();
			Thread thread2 = new Thread(new WhileOddChecker());
			thread2.start();
//			SpringApplication.run(Hw6Task1Application.class, args);
		}
	}
}

